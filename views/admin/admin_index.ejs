<%- include("common/header.ejs") %>
		<!--start page wrapper -->
		<div class="page-wrapper">
			<div class="page-content">
				<div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
					<div class="breadcrumb-title pe-3">Dashboard</div>
					<div class="ps-3">
						<nav aria-label="breadcrumb">
							<ol class="breadcrumb mb-0 p-0">
								<li class="breadcrumb-item"><a href="/"><i class="bx bx-home-alt"></i></a>
								</li>
								<li class="breadcrumb-item active" aria-current="page">Home</li>
							</ol>
						</nav>
					</div>
				</div>
				
				<!--end row-->
				
				<!--end row-->
				
				<!--end row-->
				<div class="row">
					<div class="col-xl-12 d-flex">
						<div class="card radius-10 w-100">
							<div class="card-body">
								<div class="d-flex align-items-center">
									<div>
										<h5 class="mb-1">Customers connect with you</h5>
										<!--<p class="mb-0 font-13"><i class='bx bxs-calendar'></i>in last 30 days</p>-->
									</div>
								</div>
								<div class="table-responsive mt-4">
									<table class="table align-middle mb-0 table-hover text-center" id="contact-data">
										<thead class="table-light">
											<tr>
												<th>Date & Time</th>
												<th>Customer's Name</th>
												<th>Contact Number</th>
												<th>Mail ID</th>
												<th>Messages</th>
											</tr>
										</thead>
										
										<% if(contactdata.length>0) {%>
											<tbody id="contact-history">
											  <% for(let contact of contactdata){ %>
											  <tr>
											
												<td><%= contact.created_at %></td>
												<td><%= contact.name %></td>
												<td><%= contact.email %></td>
												<td><%= contact.phone %></td>
												<td><%= contact.message %></td>
												
											  </tr>
											  
											</tbody>
											<%}}else{%>
											<h1>No Data Found</h1>
											<% } %>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--end page wrapper -->
		<!--start overlay-->
		<div class="overlay toggle-icon"></div>
		<!--end overlay-->
		<!--Start Back To Top Button--> <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
		<!--End Back To Top Button-->
		<%- include("common/footer.ejs") %>